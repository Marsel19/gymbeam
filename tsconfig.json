{
    "compilerOptions": {
      "target": "es2016",
      "lib": [
        "es6"
      ],
      "module": "commonjs",
      "rootDir": "./",
      "outDir": "./build",
      "allowJs": true,
      "esModuleInterop": true,
      "strict": true,
      "noImplicitAny": true,
      "skipLibCheck": true
    },
    "exclude": ["node_modules", "build"]
  }